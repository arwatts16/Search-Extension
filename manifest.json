{
  "name": "Search",
  "version": "0.5",
  "description": "highlights inserted key words",
  "permissions": ["webRequest", "tabs"],
  "icons": {
    "16": "images/search2.png",
    "48": "images/search1.png",
    "128": "images/search1.png"
  },
  "background": {
    "scripts": [
      "src/searchExtensionApp.js",
      "src/ISearchProvider.js",
      "src/uomfile.js",
      "src/searchResult.js",
      "src/background.js"
    ]
  },
  "options_ui": {
    "page": "views/options.html",
    "open_in_tab": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/find_highlight.js"]
    }
  ],
  "browser_action": {
    "default_popup": "views/popup.html",
    "default_title": "Search-Extension",
    "default_icon": "images/search2.png"
  },
  "manifest_version": 2
}
